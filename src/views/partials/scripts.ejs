<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
<script>
  const tallyFormId = "wdaerq"
  $(function () {
    $("#get-started").on("click", function (e) {
      e.preventDefault()
      if (localStorage.getItem("hasSubmittedInquiry") === "true") {
        window.location.href = "/inquiry-confirmation";
      } else {
        Tally.openPopup(tallyFormId, {
          onSubmit: (_) => {
            localStorage.setItem("hasSubmittedInquiry", "true")
            window.location.href = "/inquiry-confirmation";
          },
        })
      }
    })
  })
</script>